  const metricHeaders: StatsdItem[0] = {\n    type: 'statsd',\n    length: payload.length,\n  };\n  return [metricHeaders, payload];\n}\n"},"lineCount":null}},"error":null,"hash":"5678a6770dd0a0e98d64eef8f23fbfb1","cacheData":{"env":{}}}